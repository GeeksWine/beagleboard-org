		 </select>
		 <script>SynchronizeSelection(document.theForm.repository,"<% this.repository %>")</script>
	  </td>
     </tr>
     <tr>
      <td align="right">Repository URL</td>
      <td><input name="repositoryurl" type="text" size="50" value="<% this.repositoryurl %>" /></td>
     </tr>
    </table>

