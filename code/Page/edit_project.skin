<style type="text/css">
  table.membertbl 
  {
     border-style:solid; 
	 border-color: #C95701;
     display:none;
		 margin-bottom:20px
  }
</style>

<script type="text/javascript">
  numMembers = <% this.numMembers %>;
  function SynchronizeSelection(sel, val) {
     for (i=0;i<sel.options.length;i++) {
	    if (sel.options[i].value == val)
		  sel.selectedIndex = i;
	 }
  }
  function RemoveMember(userid,name,role)
  {
    userid.value = "";
	name.value = "";
	role.selectedIndex = 0;
  }
  function AddMember()
  {
    var objList;
    nCurMember = 0;

    if (numMembers >= 10) return;
    objList = document.theForm.getElementsByTagName("table");
    for (i=0;i<objList.length;i++) {
			if (objList[i].className == "membertbl") {
				if (nCurMember++ < numMembers) objList[i].style.display="block";
      }
    }
	  numMembers++;
  }  
</script>

