    <body>&lt;script src=&quot;/static/jquery.qtip.js&quot;&gt;&lt;/script&gt;
 &lt;div align=&quot;right&quot; style=&quot;float: right;&quot;&gt;
 &lt;form action=&quot;http://beagleboard.org/project/new/edit_project&quot; method=&quot;get&quot;&gt;
  &lt;input name=&quot;open&quot; type=&quot;submit&quot; value=&quot;Register Project&quot; style=&quot;width:30em; height:5em; text-align:center&quot; /&gt;
 &lt;/form&gt;
 &lt;/div&gt;
 &lt;h2 id=&quot;registered&quot;&gt;List of registered projects&lt;/h2&gt;
 &lt;script language=&quot;javascript&quot;&gt;
var update_count = function () {
 var visible = 0;
 var total = 0;
 $(&quot;#projects tbody tr:hidden&quot;).each(function () {
  total++;
 });
 $(&quot;#projects tbody tr:visible&quot;).each(function () {
  visible++;
  total++;
 });
 $(&apos;#visible_count&apos;).html(visible);
 $(&apos;#total_count&apos;).html(total);
};

$(document).ready(
 function() {
  $(&apos;#hosting&apos;).hide(&apos;fast&apos;);
  $(&apos;#search_links&apos;).hide(&apos;fast&apos;);
  var visible_count = document.createElement(&quot;span&quot;);
  visible_count.style.cssFloat=&quot;none&quot;;
  $(visible_count).attr(&quot;id&quot;, &quot;visible_count&quot;);
  var total_count = document.createElement(&quot;span&quot;);
  total_count.style.cssFloat=&quot;none&quot;;
  var show_more = document.createElement(&quot;span&quot;);
  show_more.style.cssFloat=&quot;none&quot;;
  $(total_count).attr(&quot;id&quot;, &quot;total_count&quot;);
  $(&apos;#registered&apos;).append(&apos; (showing &apos;);
  $(&apos;#registered&apos;).append(visible_count);
  $(&apos;#registered&apos;).append(&apos; of &apos;);
  $(&apos;#registered&apos;).append(total_count);
  $(&apos;#registered&apos;).append(&apos;) &apos;);
  $(&apos;#registered&apos;).append(show_more);
  update_count();
  var proj_filter = document.createElement(&quot;input&quot;);
  $(proj_filter).attr(&quot;id&quot;, &quot;proj_filter&quot;);
  $(proj_filter).attr(&quot;name&quot;, &quot;proj_filter&quot;);
  $(proj_filter).attr(&quot;size&quot;, &quot;40&quot;);  
  $(&apos;#registered&apos;).append(proj_filter);
  var indexID = 0;
  $(&apos;.wants-g-plusone&apos;).each(function() {
   $(this).attr(&apos;id&apos;, &apos;gp&apos; + indexID);
   indexID++;
  });        
  var doFilter = function() {
   var filter = $(&apos;#proj_filter&apos;).val();
   var visible = 0;
   $(&quot;#projects tbody tr&quot;).each(function () {
    if (
     ( $(this).text().search(new RegExp(filter, &quot;i&quot;)) &gt;= 0 ) 
     &amp;&amp; (visible &lt; 10)
    ) {
     $(this).show();
     visible++;
     $(this).find(&apos;.wants-g-plusone&apos;).each(function() {
      $(this).addClass(&apos;g-plusone&apos;);
      try {
       gapi.gplusone.render(&apos;&apos; + $(this).attr(&apos;id&apos;));
      } catch(ex) {
       $(this).html(&apos;&apos; + ex);
      }
     });
    } else {
     $(this).hide();
    }
   });
   addedIDs = true;
   $(&apos;#visible_count&apos;).html(visible);
  };
  doFilter();
  $(&apos;#proj_filter&apos;).keyup(doFilter);
 }
);
&lt;/script&gt;
