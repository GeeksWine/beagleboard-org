 &lt;script type=&quot;text/javascript&quot; src=&quot;http://del.icio.us/feeds/json/jadon/beagleboard+peripheral+verified+recommendation?count=20&quot;&gt;0&lt;/script&gt;
 &lt;script type=&quot;text/javascript&quot;&gt;function showImage(img){ return (function(){ img.style.display=&apos;inline&apos;; }) }

    var ul = document.createElement(&apos;ul&apos;)
    for (var i=0, post; post = Delicious.posts[i]; i++) {
        var li = document.createElement(&apos;li&apos;)
        var a = document.createElement(&apos;a&apos;)
        var img = document.createElement(&apos;img&apos;)
        img.style.position = &apos;absolute&apos;
        img.style.display = &apos;none&apos;
        img.height = img.width = 16
        img.src = post.u.split(&apos;/&apos;).splice(0,3).join(&apos;/&apos;)+&apos;/favicon.ico&apos;
        img.onload = showImage(img);
        a.setAttribute(&apos;href&apos;, post.u)
        a.setAttribute(&apos;title&apos;, post.n)
        a.appendChild(document.createTextNode(post.d))
        li.appendChild(a)
        //li.appendChild(img)
        ul.appendChild(li)
    }
    document.getElementById(&apos;delicious-container&apos;).appendChild(ul)&lt;/script&gt;
