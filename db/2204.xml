    <body>&lt;div id=&quot;rotate-items&quot; style=&apos;width: 600px; height: 400px;&apos;&gt;
 &lt;img id=&quot;a&quot; src=&quot;/static/images/bone.jpg&quot; style=&apos;height: 300px; position: absolute; left: 200px; top: 100px; z-index: 2;&apos; /&gt; 
 &lt;img id=&quot;b&quot; src=&quot;/static/images/board.jpg&quot; style=&apos;height: 80px; position: absolute; left: 50px; top: 50px; z-index: 1;&apos; /&gt; 
 &lt;img id=&quot;c&quot; src=&quot;/static/images/xm.jpg&quot; style=&apos;height: 80px; position: absolute; left: 500px; top: 50px; z-index: 1;&apos;  /&gt;
&lt;/div&gt;
&lt;p&gt;&lt;/p&gt;
&lt;script&gt;
 var onleft = {
  height: &quot;80px&quot;,
  left: &quot;50px&quot;,
  top: &quot;50px&quot;
 };
 var oncenter = {
  height: &quot;300px&quot;,
  left: &quot;200px&quot;,
  top: &quot;100px&quot;
 };
 var onright = {
  height: &quot;80px&quot;,
  left: &quot;500px&quot;,
  top: &quot;100px&quot;
 };
 doRotate = function() {
  if($(&apos;#a&apos;).css(&apos;left&apos;) == onleft.left) {
   $(&apos;#a&apos;).animate(onright, 1000);
   $(&apos;#b&apos;).animate(onleft, 1000);
   $(&apos;#c&apos;).animate(oncenter, 1000);
  } else  if($(&apos;#a&apos;).css(&apos;left&apos;) == oncenter.left) {
   $(&apos;#a&apos;).animate(onleft, 1000);
   $(&apos;#b&apos;).animate(oncenter, 1000);
   $(&apos;#c&apos;).animate(onright, 1000);
  } else {
   $(&apos;#a&apos;).animate(oncenter, 1000);
   $(&apos;#b&apos;).animate(onright, 1000);
   $(&apos;#c&apos;).animate(onleft, 1000);
  }
 };
 setInterval(doRotate, 5000);
&lt;/script&gt;</body>
