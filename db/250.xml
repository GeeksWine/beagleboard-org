 &lt;h1&gt;Return Merchandise Application&lt;/h1&gt;
 &lt;form action=&quot;mailto:rma@beagleboard.org?subject=BeagleBoard RMA request for serial number SERIAL_NUM_HERE_TOO&quot; method=&quot;post&quot; enctype=&quot;text/plain&quot;&gt;
  &lt;table&gt;
   &lt;tbody&gt;
    &lt;tr&gt;
     &lt;td&gt;Serial #&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;serial_num&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Revision&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;rev&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Name&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;name&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Company&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;company&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Address&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;address&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Phone #&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;phone_num&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Email&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;email&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Purchased from&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;distributor&quot; type=&quot;text&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Replacement OK&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;replace&quot; type=&quot;checkbox&quot; value=&quot;yes&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Description of issue&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;issue&quot; type=&quot;text&quot; size=&quot;100&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt;Return method&lt;/td&gt;
     &lt;td&gt;
      &lt;input name=&quot;ship_method&quot; type=&quot;radio&quot; value=&quot;USPS Mail&quot;/&gt;
      USPS Mail
      &lt;input name=&quot;ship_method&quot; type=&quot;radio&quot; value=&quot;UPS&quot;/&gt;
      UPS
      &lt;input name=&quot;ship_method&quot; type=&quot;radio&quot; value=&quot;DHL&quot;/&gt;
      DHL
      &lt;input name=&quot;ship_method&quot; type=&quot;radio&quot; value=&quot;FedEX&quot;/&gt;
      FedEX
     &lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
     &lt;td&gt; &lt;/td&gt;
     &lt;td&gt;
      &lt;input type=&quot;submit&quot; value=&quot;Submit&quot;/&gt;
     &lt;/td&gt;
    &lt;/tr&gt;
   &lt;/tbody&gt;
  &lt;/table&gt;
 &lt;/form&gt;
 &lt;h2&gt;Return Address&lt;/h2&gt;
 &lt;p&gt;
  Beagle Hospital
  &lt;br/&gt;
  675 North Glenville #195
  &lt;br/&gt;
  Richardson, TX 75081
 &lt;/p&gt;
