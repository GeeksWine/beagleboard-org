function slideBox() {
    if ( arguments.callee._singletonInstance )
        return arguments.callee._singletonInstance;
    arguments.callee._singletonInstance = this;
    
    this.createBoxes = function() {
        $(&apos;#slider&apos;).movingBoxes({
            startPanel   : 1,
            reducedSize  : 0.8,
            wrap         : true,
            buildNav     : true,
            navFormatter : function(){ return &quot;&amp;#9679;&quot;; },
            width        : 250,
            height       : 300,
            fixedHeight  : true,
            panelWidth   : 0.9,
            initAnimation: true,
            wrap         : true,
            speed        : 500,
            completed    : this.startTimeout
        });
    }

    this.nextSlide = function() { 
        var mb = $(&apos;#slider&apos;).data(&apos;movingBoxes&apos;);
        mb.goForward();
        if(this.timeout) {
            clearTimeout(this.timeout);
            this.timout = false;
        }
    };
    
    this.startTimeout = function() {
        if(this.timeout) {
            clearTimeout(this.timeout);
            this.timeout = false;
        }
        this.timeout = setTimeout(this.nextSlide, 5000);
    };
}

